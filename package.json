{
  "name": "autobox-engine-ts",
  "version": "1.0.0",
  "description": "TypeScript implementation of Autobox simulation runtime",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --clear-screen=true src/index.ts --config=./examples --simulation-name=gift_choice",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:cli": "node dist/index.js --cli",
    "test": "jest --runInBand",
    "test:unit": "jest tests/unit --runInBand",
    "test:integration": "jest tests/integration --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --runInBand",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:build": "./bin/docker-build",
    "docker:build:dev": "./bin/docker-build --dev",
    "docker:run": "./bin/docker-run",
    "docker:run:dev": "./bin/docker-run-dev",
    "docker:run:exit": "./bin/docker-run-and-exit",
    "docker:clean": "./bin/docker-clean",
    "docker:clean:all": "./bin/docker-clean --all --force"
  },
  "keywords": [
    "autobox",
    "simulation",
    "typescript"
  ],
  "author": "margostino <maj.dagostino@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/swagger-ui-express": "^4.1.8",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.7",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "ioredis": "5.4.2",
    "ioredis-timeout": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^6.2.0",
    "prom-client": "^15.1.3",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-transport": "^4.7.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-util": "^29.7.0",
    "junit-merge": "^2.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@4.9.3+sha512.8295ee814f6b253e16f516416481b481a215ed03ef1ae38524d108084872560a9ed75aeb23b91ab64222062ac4149a594150ae538c2a9536fdbcefd4e49b11cc"
}
